  },
  "dependencies": {
    "@reason-native/rely": "*",
    "ocaml": "*",
    "@esy-ocaml/reason": "*"
  },
  "devDependencies": {
    "ocaml": "*",
    "@opam/merlin": "*",
    "@opam/dune": "*",
    "refmterr": "*",
    "@esy-ocaml/reason": "*"
  }
}
